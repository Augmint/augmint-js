{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "new-parens": true,
        "ordered-imports": {
            "severity": "warn"
        },
        "jsdoc-format": {
            "severity": "warn"
        },
        "object-literal-sort-keys": false,
        "radix": false,
        "no-console": {
            "severity": "warn",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    }
}