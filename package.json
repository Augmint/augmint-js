{
  "name": "@augmint/js",
  "version": "0.0.2",
  "description": "Augmin Javascript Library",
  "keywords": [
    "augmint javascript library A-EUR stablecoin web3 dapp"
  ],
  "author": "Augmint",
  "license": "AGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Augmint/augmint-ratesfeeder.git"
  },
  "bugs": {
    "url": "https://github.com/Augmint/augmint-js/issues"
  },
  "homepage": "https://github.com/Augmint/augmint-js#readme",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "^10.15.3",
    "yarn": "^1.15.2"
  },
  "dependencies": {
    "bignumber.js": "5.0.0",
    "dotenv": "7.0.0",
    "ulog": "2.0.0-beta.6",
    "web3": "1.0.0-beta.36"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "cross-env": "5.2",
    "eslint": "5.16.0",
    "mocha": "6.1.2",
    "sinon": "7.3.1"
  },
  "scripts": {
    "test": "yarn cross-env NODE_ENV=test NODE_PATH=./src yarn mocha 'src/**/*.test.js' --exit",
    "contracts:migrate": "cd augmint-contracts && yarn migrate",
    "ganache:run": "cd augmint-contracts && yarn ganache:run",
    "contracts:runmigrate": "cd augmint-contracts && yarn runmigrate"
  },
  "greenkeeper": {
    "ignore": [
      "bignumber.js"
    ]
  }
}
